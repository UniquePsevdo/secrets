{
  "name": "client",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "webpack-dev-server --port=4200",
    "local": "ng serve --environment=local",
    "dev-build": "ng build --dev --output-path=builds/dev-build",
    "prod-build": "ng build --prod --aot --no-sourcemap --output-path=builds/prod-build",
    "univ-build": "ng build --prod --aot=false --app universal --verbose --output-hashing=none --output-path=builds/univ-build/browser",
    "bundle-report": "webpack-bundle-analyzer builds/prod-build/stats.json",
    "ngc:server": "ngc -p src/tsconfig.server.json",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "test": "karma start ./karma.conf.js",
    "lint": "ng lint",
    "e2e": "protractor ./protractor.conf.js",
    "build": "webpack",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet"
  },
  "private": true,
  "dependencies": {
